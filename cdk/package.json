{
  "name": "cdk-app-sync-chat",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "cdk:deploy:stateless": "cdk deploy --require-approval=never StatelessStack",
    "cdk:deploy:stateful": "cdk deploy --require-approval=never StatefulStack",
    "cdk:deploy:all": "cdk deploy --require-approval=never --all",
    "cdk:destroy:stateless": "cdk destroy --require-approval=never StatelessStack",
    "cdk:destroy:stateful": "cdk destroy --require-approval=never StatefulStack",
    "cdk:destroy:all": "cdk destroy --require-approval=never --all"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "22.13.5",
    "aws-cdk": "2.1001.0",
    "esbuild": "^0.25.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.3"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.181.1",
    "cargo-lambda-cdk": "^0.0.32",
    "constructs": "^10.4.2",
    "source-map-support": "^0.5.21"
  }
}
